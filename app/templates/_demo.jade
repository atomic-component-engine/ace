//-
	@file Demo page for the <%= componentName %> module
	@author <%= name %> <%= email %>
	@extends page_templates/_factory.jade
	@requires mixins/<%= componentName %>/[MODFILE]

//- Page dependencies
include mixins/<%= componentType %>s/<%= id %>

//- Page template
extends page_templates/_factory

//- Page variables to pass to template
block page_vars
	- var page = {}
	- page.id = "<%= id %>"
	- page.title = "<%= id %>"
	- page.mainScript = "<%= id %>.js"


//- Content block
block content

	+<%= id %>()
